{
  "fontsAlias": {
     "stdBold":  "DejaVu Sans-Bold",
     "stdBoldItalic":  "DejaVu Sans-BoldOblique",
     "stdFont":  "DejaVu Sans",
     "stdItalic":  "DejaVu Sans-Oblique",
     "stdMono":  "DejaVu Sans Mono",
     "stdMonoBold":  "DejaVu Sans Mono-Bold",
     "stdMonoBoldItalic":  "DejaVu Sans Mono-BoldOblique",
     "stdMonoItalic":  "DejaVu Sans Mono-Oblique",
     "stdSans":  "DejaVu Sans",
     "stdSansBold":  "DejaVu Sans-Bold",
     "stdSansBoldItalic":  "DejaVu Sans-BoldOblique",
     "stdSansItalic":  "DejaVu Sans-Oblique",
     "stdSerif": "DejaVu Serif"
  },
  "pageSetup": {
    "firstTemplate": "coverPage",
    "size": "A5",
    "margin-top": "1cm",
    "margin-bottom": "1cm",
    "margin-left": "1cm",
    "margin-right": "1cm",
    "margin-gutter": "0cm",
    "spacing-header": "5mm",
    "spacing-footer": "5mm"
  },
  "pageTemplates" : {
    "coverPage": {
        "frames": [
            ["0cm", "0cm", "100%", "100%"]
          ],
        "background" : "cover.pdf",
        "showHeader" : false,
        "showFooter" : false
    },
    "oneColumn": {
        "defaultHeader": "###Section###",
        "defaultFooter": "###Page###"
    }
  },  
  "styles" : [
    [ "base", {
        "fontName": "stdSerif"
      }],
    [ "blockquote", {
        "alignment": "left",
        "fontName": "stdItalic",
        "width": "70%"
      }],
    [ "attribution", {
        "fontName": "stdBold"
      }],
    [ "code" , {
        "borderWidth": 0,
        "borderPadding": 0,
        "borderColor": null,
        "borderRadius": null,
        "backColor": "#00000000"
      }],
    [ "listado" , {
        "parent": "code"
      }],
    [ "cctable" , {
        "parent": "bodytext",
        "commands": [
            ["VALIGN", [0,0], [-1,-1], "middle"]
            ]
      }],
    ["endnote", {
        "parent": "bodytext",
        "commands": [
            ["VALIGN", [ 0, 0 ], [ -1, -1 ], "TOP" ],
            ["BOTTOMPADDING", [ 0, 0 ], [ -1, -1 ], 0 ],
            ["RIGHTPADDING", [ 0, 0 ], [ 1, -1 ], 0 ],
            ["LINEAFTER", [0,0],[0,-1], 1. , "#C0C0C0"]
	],
        "colWidths": ["1cm", null]
    }],
    [ "titulo-listado" , {
        "parent": "bodytext",
        "fontName": "stdBold",
        "alignment": "right",
        "spaceBefore": 0
      }]
]}
